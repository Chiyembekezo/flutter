defines = -DNDEBUG -DSK_GAMMA_APPLY_TO_A8 -DSK_GL -DSK_USE_LIBGIFCODEC -DSK_CODEC_DECODES_PNG -DSK_ENCODE_PNG -DSK_R32_SHIFT=16
framework_dirs =
include_dirs = -I../../tools/timer -I../../include/third_party/vulkan -I../../modules/particles/include -I../.. -I../../third_party/externals/libgifcodec -I../.. -I../../modules/skresources/include
cflags = -Wno-attributes -fstrict-aliasing -fPIC -fvisibility=hidden -O3 -fdata-sections -ffunction-sections -Wno-sign-conversion -Wno-unused-parameter
cflags_cc = -std=c++17 -fvisibility-inlines-hidden -fno-exceptions -fno-rtti
root_out_dir = .
target_output_name = libparticles

build obj/modules/particles/src/libparticles.SkParticleBinding.o: cxx ../../modules/particles/src/SkParticleBinding.cpp
build obj/modules/particles/src/libparticles.SkParticleDrawable.o: cxx ../../modules/particles/src/SkParticleDrawable.cpp
build obj/modules/particles/src/libparticles.SkParticleEffect.o: cxx ../../modules/particles/src/SkParticleEffect.cpp
build obj/modules/particles/src/libparticles.SkReflected.o: cxx ../../modules/particles/src/SkReflected.cpp

build ./libparticles.a: alink obj/modules/particles/src/libparticles.SkParticleBinding.o obj/modules/particles/src/libparticles.SkParticleDrawable.o obj/modules/particles/src/libparticles.SkParticleEffect.o obj/modules/particles/src/libparticles.SkReflected.o || ./libskia.a ./libskresources.a
  arflags =
  output_extension = .a
  output_dir = 
